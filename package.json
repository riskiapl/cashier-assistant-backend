{
  "name": "cashier-assistant-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate:createdb": "sequelize-cli db:create",
    "migrate:create": "sequelize-cli model:generate --name member --attributes username:string,password:string, created_at:date",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undoall": "sequelize-cli db:migrate:undo:all",
    "migrate:createmodel": "sequelize-auto -o \"./models\" -d cashier_assistant -h localhost -u postgres -p 5432 -x password -e postgres"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "sequelize-auto": "^0.8.8"
  }
}
